    "path": "^0.12.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/pages/components/*.js": [
      "npm run lint:fix",
      "git add"
